{
  "name": "dashboardami",
  "version": "0.1.0",
  "private": true,
  "author": "Jacobo Rodrigo",
  "main": "public/electron/electron.js",
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "daisyui": "^1.11.0",
    "electron-is-dev": "^2.0.0",
    "mongoose": "^5.13.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-responsive-carousel": "^3.2.20",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toast": "^1.0.3",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.1",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "full": "yarn build:electron && yarn electron-pack",
    "dev": "concurrently \"yarn dev:watch\" \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\" ",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "preelectron-pack": "yarn build",
    "build:electron": "tsc --project tsconfig.electron.json",
    "dev:watch": "tsc --project tsconfig.electron.json --watch",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron/electron.js",
    "electron-dev": "concurrently  \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/jest": "^26.0.24",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-transition-group": "^4.4.2",
    "@types/redux": "^3.6.0",
    "autoprefixer": "^9",
    "concurrently": "^6.2.1",
    "electron": "9.2.0",
    "electron-builder": "^22.11.7",
    "postcss": "^7",
    "redux-devtools-extension": "^2.13.9",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.7",
    "wait-on": "^6.0.0"
  },
  "build": {
    "appId": "com.jacobo.ami",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
